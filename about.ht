.skills-container {
  display: flex;
  gap: 2rem;
}

.skill-card {
  background-color: white;
  border-radius: 8px;
  padding: 1.5rem;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
  text-align: center;
}

.circle-progress {
  width: 150px;
  height: 150px;
  border-radius: 50%;
  background: conic-gradient(
    #4169e1 var(--percentage),
    #f0f0f0 var(--percentage)
  );
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 1rem auto;
}

.percentage {
  font-size: 2rem;
  font-weight: bold;
}

.progress-info {
  display: flex;
  justify-content: space-between;
  margin-top: 1rem;
}

.progress-info div {
  text-align: center;
}

.progress-info span {
  font-weight: bold;
}

.progress-info p {
  color: #888;
  font-size: 0.8rem;
  margin: 0;
}



